{
	"semi": true,
	"singleQuote": true,
	"printWidth": 100,
	"tabWidth": 4,
	"useTabs": false,
	"trailingComma": "es5",
	"bracketSpacing": true,
	"arrowParens": "always",
	"vueIndentScriptAndStyle": true,
	"embeddedLanguageFormatting": "auto",
	"htmlWhitespaceSensitivity": "ignore",
	"plugins": ["prettier-plugin-tailwindcss"]
}
